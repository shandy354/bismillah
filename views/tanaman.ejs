<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tanaman</title>
    <%- include('./partials/head') %>
</head>
<header>
    <%- include("./partials/navHome") %>
</header>
<body>
    <div class="cotainer mt-100 ">
        <div class="row justify-content-center">
                <div class="row row-cols-1 row-cols-md-4 g-4">
                  <% tanaman.forEach(tanamans => { %>
                  <div class="col">
                    <div class="card h-100">
                      <img src="/images/<%= tanamans.img %>" class="card-img-top" alt="..." >
                      <div class="card-body">
                        <h5 class="card-title"><%= tanamans.name %></h5>
                        <!-- <h5>Kategori <%= tanamans.kategori %></h5>
                        <p class="card-text"><%= tanamans.deskripsi %></p> -->
                      </div>
                      
                      <div class="card-footer">
                        <div class="col-md-12 text-center">
                          <a href="/tanaman/<%= tanamans.id %>" class="btn btn-primary ">DETIL</a> 
                        </div>
                      </div>
                    </div>
                  </div>
                  <% }) %> 
                </div> 
        </div>

    </div>
</body>
</html>